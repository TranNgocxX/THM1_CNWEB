CREATE DATABASE QLCH;
USE QLCH;

CREATE TABLE products (
    id INT AUTO_INCREMENT PRIMARY KEY,
    name VARCHAR(50) NOT NULL,
    price INT NOT NULL
);

INSERT INTO products (name, price) VALUES
 ('Sản phẩm 1', 1000), 
('Sản phẩm 2', 2000), 
('Sản phẩm 3', 3000), 
('Sản phẩm 4', 4000), 
('Sản phẩm 5', 5000);
